@Pass
FS_PREC_FLOAT(mediump)
varying vec2 vTexCoord;
#ifdef VS
attribute vec3 position;
attribute vec2 texCoord;
uniform mat4 _matPVM;
void main()
{
  gl_Position = _matPVM * vec4(position,1.0);
  vTexCoord = texCoord;
}
#endif
#ifdef FS
@{type:"color",default:"white"}
uniform vec4 color;
void main(void)
{
    gl_FragColor = color;
}
#endif
