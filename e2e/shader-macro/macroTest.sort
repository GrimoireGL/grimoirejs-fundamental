@Pass{
  FS_PREC(mediump,float)
  @ExposeMacro(int,TEST,test,3)
  #ifdef VS
  attribute vec3 position;

    void main(){
      gl_Position = vec4(position,1.0);
    }
  #endif

  #ifdef FS
    void main(){
      vec3 color = vec3(0);
      for(int i = 0; i < TEST; i++){
        color += vec3(0.2,0,0);
      }
      gl_FragColor = vec4(color,1);
    }
  #endif
}
